# File created: 2011-08-06 16:04:23

include_rules

: config.h.in            |> tup varsed %f %o |> config.h
: foreach *.c | config.h |> $(CC) $(CFLAGS) -c %f -o %o |> %B.o
: *.o                    |> $(CC) $(LINKFLAGS) -shared %f -o %o |> libmushspace.so
: *.o                    |> ar rcs %o %f |> libmushspace.a

.gitignore
