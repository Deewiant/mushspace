# File created: 2011-08-06 16:04:23

include_rules

: config.h.in |> ^ VARSED %o^ tup varsed %f %o |> config.h

: foreach *.all.c  | config.h |> !cc1d |> %B.1d.o
: foreach *.all.c  | config.h |> !cc2d |> %B.2d.o
: foreach *.all.c  | config.h |> !cc3d |> %B.3d.o
: foreach *.all.c  | config.h |> !cc93 |> %B.93.o
: foreach *.any.c  | config.h |> !cc   |> %B.o
: foreach *.both.c | config.h |> !cc98 |> %B.98.o
: foreach *.both.c | config.h |> !cc93 |> %B.93.o
: foreach *.93.c   | config.h |> !cc93 |> %B.o
: foreach *.98.c   | config.h |> !cc1d |> %B.1d.o
: foreach *.98.c   | config.h |> !cc2d |> %B.2d.o
: foreach *.98.c   | config.h |> !cc3d |> %B.3d.o

: *.o |> !ccshared |> libmushspace.so
: *.o |> !ar |> libmushspace.a

.gitignore
